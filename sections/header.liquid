<header class="site-header">
  <div class="logo">
    <a href="{{ shop.url }}">
      <img width="30px" height="30px" src="{{ 'logo.jpeg' | asset_url }}" alt="{{ shop.name }}">
    </a>
  </div>
  <nav class="navigation">
    <ul>
      {% for link in linklists['main-menu'].links %}
        <li>
          <a href="{{ link.url }}">{{ link.title }}</a>
        </li>
      {% endfor %}
    </ul>
  </nav>
</header>
